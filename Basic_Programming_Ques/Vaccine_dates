#include <bits/stdc++.h>
#define ll long long int
using namespace std;

int main() {
	// your code goes here
	ll t;
	cin>>t;
	while(t--){
	    ll d,l,r;
	    cin>>d>>l>>r;
	    
	    if(d<l){
	        cout<<"Too Early"<<endl;
	        }
	        else if(d>=l && d<=r){
	            cout<<"Take second dose now"<<endl;
	        }else
	        {
	            cout<<"Too Late"<<endl;
	        }
	}
	return 0;
}

