#include <bits/stdc++.h>
#define ll long long int
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    string s1,s2;
	    cin>>s1>>s2;
	    int cnt=0;
	    for(int i=0;i<s1.length();i++){
	        if(s1[i]==s2[i])
	        cnt++;
	        else if(s1[i]=='?' || s2[i]=='?'){
	            cnt++;
	       
	        }
	    }
	    if(cnt==s1.length()){
	        cout<<"Yes"<<endl;
	    }else{
	        cout<<"No"<<endl;
	    }
	}
	return 0;
}
