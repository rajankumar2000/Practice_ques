#include <bits/stdc++.h>
#define ll long long int
using namespace std;

int main() {
	// your code goes here
	ll t;
	cin>>t;
	while(t--){
	    string s;
	    cin>>s;
	    for(ll i=0;i<s.length()-4;i++){
	        if(s.substr(i,5)=="party"){
	            s[i+2]='w';
	            s[i+3]='r';
	            s[i+4]='i';
	        }
	    }
	    cout<<s<<endl;
	}
	return 0;
}
